NODE_PATH=src
EXTEND_ESLINT=true

# Version
# see package.json, set env before starting or building the app
REACT_APP_REPO_UI_GIT_SHA=
REACT_APP_REPO_UI_URL=https://github.com/homura/force-bridge-ui

# Force-Bridge RPC
REACT_APP_BRIDGE_RPC_URL=
# CKB node RPC
REACT_APP_CKB_RPC_URL=
# 0: mainnet
# 1: testnet
# 2: devnet
REACT_APP_CKB_CHAIN_ID=1

# pwlock config if using devnet
REACT_APP_PWLOCK_OUTPOINT_TXHASH=
REACT_APP_PWLOCK_OUTPOINT_INDEX=
REACT_APP_PWLOCK_DEP_TYPE=
REACT_APP_PWLOCK_CODE_HASH=
REACT_APP_PWLOCK_HASH_TYPE=

# Nervos explorer for exploring transaction
REACT_APP_TX_EXPLORER_NERVOS=https://explorer.nervos.org/aggron/transaction/
# Ethereum explorer for exploring transaction
REACT_APP_TX_EXPLORER_ETHEREUM=https://rinkeby.etherscan.io/tx/

# Rinkeby
REACT_APP_ETHEREUM_ENABLE_CHAIN_ID=4
REACT_APP_ETHEREUM_ENABLE_CHAIN_NAME=Rinkeby

# Godwoken
REACT_APP_GODWOKEN_ETH_LOCKHASH=
REACT_APP_GODWOKEN_ROLLUP_TYPEHASH=
